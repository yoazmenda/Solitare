	ms_y = stat(33)
end

function _draw()
	cls()

	-- draw background
	map(0, 0, 0, 0, 16, 16)
	
	-- draw card
	render_all_cards()
	
	-- draw mouse
	spr(1, ms_x, ms_y)
end 


